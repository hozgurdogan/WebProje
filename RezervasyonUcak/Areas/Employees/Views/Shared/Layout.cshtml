@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers


<!DOCTYPE html>
<html lang="en">
<head>
	<script type="text/javascript" src='https://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.8.3.min.js'></script>
	<script type="text/javascript" src='https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.0.3/js/bootstrap.min.js'></script>


    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - RezervasyonUcak</title>
	
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/employeeHomeScreen.css" asp-append-version="true" />

	
	
  

	<!--- 
	<script src="~/lib/jquery/dist/jquery.min.js"></script> 
	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>--->
	<script src="~/js/site.js" asp-append-version="true"></script>	
</head>
<body>
    <header>


			<!---  --->
			<nav class="navbar navbar-expand-lg">
				<div class="container">
					<a class="navbar-brand text-white" href="#"><i class="fa fa-graduation-cap fa-lg mr-2"></i>BLOG</a>
					<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#nvbCollapse" aria-controls="nvbCollapse">
						<span class="navbar-toggler-icon"></span>
					</button>
					<div class="collapse navbar-collapse" id="nvbCollapse">
						<ul class="navbar-nav ml-auto">
							<li class="nav-item pl-1">
							<a class="nav-link" asp-action="Anasayfa" asp-controller="Musteri"><i class="fa fa-home fa-fw mr-1"></i>Anasayfa</a>
							</li>
						
							<li class="nav-item pl-1">

							
							<a class="nav-link" asp-action="BiletlerGetir" asp-controller="Musteri"></i>Biletlerim</a>
							</li>
							<li class="nav-item pl-1">
								<a class="nav-link" asp-action="HesapBilgileri" asp-controller="Musteri" ><i class="fa fa-phone fa-fw fa-rotate-180 mr-1"></i>Hesap Bilgilerim</a>
							</li>
						
							<li class="nav-item pl-1">
							<a class="nav-link" asp-action="Logout" asp-controller="Musteri"><i class="fa fa-sign-in fa-fw mr-1"></i>Çıkış Yap</a>
							</li>
						</ul>
					</div>
				</div>
			</nav>
		


	



	</header>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
        </div>


  
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
